<tr class="cart__produit-content">
    <td class="cart__td"><img class="cart__produit-img" src="../assets/{{image}}" alt=""></td>
    <td class="cart__td">{{name}}</td>
    <td class="cart__td">{{price}} €</td>
    <td class="cart__td"><input type="number" class="produit__qte" min="0" max="{{qtemax}}" value="{{quantite}}"></td>
    <td class="cart__td">{{total}} €</td>
</tr>